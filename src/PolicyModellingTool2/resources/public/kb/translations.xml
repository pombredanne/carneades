<?xml version="1.0" encoding="UTF-8"?>
<translations>
  <predicate pred="hatAnspruchOeffentlicheZugaenglichmachung" args="2">
    <statements>
      <format lang="de">
        <text>%s hat einen Anspruch auf oeffentliche Zugaenglichmachung des Werkes %s.</text>
      </format>
      <format lang="en">
        <text>%s has the right to make %s available to the public.</text>
      </format>
    </statements>
  </predicate>
  <predicate pred="hatName" args="1">
    <statements>
      <format lang="de">
        <text>Der potentielle Nutzer heißt %s.</text>
      </format>
      <format lang="en">
        <text>The potential user is called %s.</text>
      </format>
    </statements>
    <question arg="0" type="text">
      <format lang="de">
        <text>Wie heißt der Rechtsinhaber?</text>
      </format>
      <format lang="en">
        <text>What is the name of the rights owner?</text>
      </format>
      <hint lang="en">Please enter the name of rights owner.</hint>
      <category>Name</category>
      <qrefs>
        <qref pred="betrifftWerk" arg="0">
        </qref>
      </qrefs>
    </question>
  </predicate>
  <predicate pred="betrifftWerk" args="1">
    <statements>
      <format lang="de">
        <text>Das betreffende Werk heißt %s.</text>
      </format>
    </statements>
    <question arg="0" type="text">
      <format lang="de">
        <text>Wie heißt das betroffene Werk?</text>
      </format>
      <format lang="en">
        <text>What is the name of the concerned orphaned work?</text>
      </format>
      <hint lang="en">Please enter a name for the orphaned work.</hint>
      <category>Work</category>
    </question>
  </predicate>
  <predicate pred="zumZweck" args="2">
    <statements>
      <format lang="de">
        <text>%s wird zum Zweck %s verwendet.</text>
      </format>
    </statements>
    <question arg="1" type="radio">
      <format lang="de">
        <text>Zu welchem Zweck wollen sie das Werk %s verwenden?</text>
      </format>
      <format lang="en">
        <text>For which purpose would you like to use %s?</text>
      </format>
      <hint lang="en">Please enter if you want to use the orphaned work for commercial purposes.</hint>
      <category>Purpose</category>
      <answers>
        <text lang="en">commercial</text>
        <text lang="en">non-commercial</text>
      </answers>
      <formalanswers>
        <text>commercial</text>
        <text>non_commercial</text>
      </formalanswers>
    </question>
  </predicate>
  <predicate pred="UrheberSuche" args="1">
    <statements>
      <format lang="de">
        <text>Die Suche nach dem Urheber war %s.</text>
      </format>
    </statements>
    <question arg="0" type="select">
      <format lang="de">
        <text>Erfolgte eine Standard- oder eine professionelle Suche?</text>
      </format>
      <format lang="en">
        <text>Is the search that has been carried out a professional one or a standard search?</text>
      </format>
      <hint lang="en">Please enter if your documented search was a professional one.</hint>
      <category>Search</category>
      <formalanswers>
        <text>Standard</text>
        <text>Professional</text>
        <text>None</text>
      </formalanswers>
      <answers>
        <text lang="en">Standardyyyy</text>
        <text lang="en">Professional</text>
        <text lang="en">None</text>
      </answers>
      <qrefs>
        <qref pred="Bekanntmachung" arg="0">
        </qref>
      </qrefs>
    </question>
  </predicate>
  <predicate pred="Bekanntmachung" args="1">
    <statements>
      <format lang="de">
        <text>%s</text>
      </format>
    </statements>
    <question arg="0" type="radio">
      <format lang="de">
        <text>Erfolgte eine Bekanntmachung der Suche?</text>
      </format>
      <format lang="en">
        <text>Did you announce the search?</text>
      </format>
      <hint lang="en">Please enter if a public announcement was carried out.</hint>
      <category>Bekanntmachung</category>
      <formalanswers>
        <text>No</text>
        <text>Public</text>
      </formalanswers>
      <answers>
        <text lang="en">No</text>
        <text lang="en">Public</text>
      </answers>
    </question>
  </predicate>
</translations>

